<div class="bio-item">
  <label>Född: </label><span>{{model.born}}</span>
</div>

{{#each info.bio as |bio|}}
  <div class="bio-item">
    <label>{{bio.kod}}: </label><span>{{bio.uppgift}}</span>
  </div>
{{/each}}

{{#with info.mail}}
  <div class="bio-item">
    <label>Email: </label><span>{{info.mail}}</span>
  </div>
{{/with}}

{{#with info.webpage}}
  <div class="bio-item">
    <label>Websida:  </label><span><a href={{info.webpage}} target="_BLANK">{{info.webpage}}</a></span>
  </div>
{{/with}}

<p>Kandiderar i nästa val: {{info.candidateNextElection}}</p>